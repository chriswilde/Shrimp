void setup() {
  pinMode(13, OUTPUT);
}

void loop() {

  for (int counter=1; counter<=4; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);}

    delay(1500);

  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);}
  
  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);}
    
    delay(1500);

    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);
  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);}

    delay(3500);

    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);
    
    delay(1500);
    
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);
    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);

    delay(1500);
    
  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(1500);
    digitalWrite(13, LOW);
    delay(500);}
    
    delay(1500);
    
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);
    
    delay(3500);

  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);}
    
    delay(1500);
    
  for (int counter=1; counter<=2; counter = counter+1){
    digitalWrite(13, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    delay(500);}

    delay(3500);    
}
